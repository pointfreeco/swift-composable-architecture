tifier"},{"text":" == ","kind":"text"},{"kind":"typeIdentifier","text":"Destination"},{"text":".","kind":"text"},{"preciseIdentifier":"s:22ComposableArchitecture7ReducerP6ActionQa","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/Action","kind":"typeIdentifier","text":"Action"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Destination"},{"kind":"text","text":" : "},{"text":"Reducer","kind":"typeIdentifier","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP"},{"text":"\n","kind":"text"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"parameters":[{"name":"toStackState","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A writable key path from parent state to a stack of destination state."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"A case path from parent action to a stack action.","type":"text"}]}],"name":"toStackAction"},{"content":[{"inlineContent":[{"type":"text","text":"A reducer that will be invoked with destination actions against elements of"},{"type":"text","text":" "},{"type":"text","text":"destination state."}],"type":"paragraph"}],"name":"destination"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The fileID."}]}],"name":"fileID"},{"name":"filePath","content":[{"inlineContent":[{"text":"The filePath.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The line."}]}],"name":"line"},{"name":"column","content":[{"type":"paragraph","inlineContent":[{"text":"The column.","type":"text"}]}]}],"kind":"parameters"},{"kind":"content","content":[{"anchor":"return-value","type":"heading","text":"Return Value","level":2},{"inlineContent":[{"text":"A reducer that combines the destination reducer with the parent reducer.","type":"text"}],"type":"paragraph"}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This version of "},{"code":"forEach","type":"codeVoice"},{"text":" works when the parent domain holds onto the child domain using","type":"text"},{"type":"text","text":" "},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackState","isActive":true,"type":"reference"},{"text":" and ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"text":"For example, if a parent feature models a navigation stack of child features using the","type":"text"},{"text":" ","type":"text"},{"type":"reference","isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackState"},{"type":"text","text":" and "},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction","isActive":true},{"text":" types, then it can perform its core logic ","type":"text"},{"inlineContent":[{"text":"and","type":"text"}],"type":"emphasis"},{"text":" the logic","type":"text"},{"text":" ","type":"text"},{"text":"of each child feature using the ","type":"text"},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" operator:"}]},{"code":["@Reducer","struct ParentFeature {","  struct State {","    var path = StackState<Path.State>()","    \/\/ ...","  }","  enum Action {","    case path(StackActionOf<Path>)","    \/\/ ...","  }","  var body: some ReducerOf<Self> {","    Reduce { state, action in","      \/\/ Core parent logic","    }","    .forEach(\\.path, action: \\.path) {","      Path()","    }","  }","}"],"type":"codeListing","syntax":"swift"},{"inlineContent":[{"text":"The ","type":"text"},{"code":"forEach","type":"codeVoice"},{"type":"text","text":" operator does a number of things to make integrating parent and child features"},{"type":"text","text":" "},{"text":"ergonomic and enforce correctness:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"It forces a specific order of operations for the child and parent features:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"When a "},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/element(id:action:)"},{"type":"text","text":" action is sent it runs the"},{"type":"text","text":" "},{"text":"child first, and then the parent. If the order was reversed, then it would be possible","type":"text"},{"type":"text","text":" "},{"type":"text","text":"for the parent feature to "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" out the child state, in which case the child feature"},{"type":"text","text":" "},{"type":"text","text":"would not be able to react to that action. That can cause subtle bugs."}]}]},{"content":[{"inlineContent":[{"text":"When a ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/popFrom(id:)"},{"type":"text","text":" action is sent it runs the parent feature"},{"text":" ","type":"text"},{"text":"before the child state is popped off the stack. This gives the parent feature an","type":"text"},{"type":"text","text":" "},{"text":"opportunity to inspect the child state one last time before the state is removed.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"When a ","type":"text"},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/push(id:state:)","type":"reference","isActive":true},{"type":"text","text":" action is sent it runs the parent feature"},{"text":" ","type":"text"},{"text":"after the child state is appended to the stack. This gives the parent feature an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"opportunity to make extra mutations to the state after it has been added."}]}]}]}]},{"content":[{"inlineContent":[{"type":"text","text":"It automatically cancels all child effects when it detects the childâ€™s state is removed"},{"type":"text","text":" "},{"text":"from the stack","type":"text"}],"type":"paragraph"}]},{"content":[{"inlineContent":[{"text":"It gives the child feature access to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DismissEffect","isActive":true},{"text":" dependency, which allows the","type":"text"},{"text":" ","type":"text"},{"text":"child feature to dismiss itself without communicating with the parent.","type":"text"}],"type":"paragraph"}]}],"type":"unorderedList"}]}],"references":{"doc://ComposableArchitecture/documentation/ComposableArchitecture/Reducer/Action":{"required":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/Action","title":"Action","role":"symbol","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"A type that holds all possible actions that cause the "},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/State","type":"reference","isActive":true},{"text":" of the reducer to change","type":"text"},{"type":"text","text":" "},{"text":"and\/or kick off a side ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Effect"},{"text":" that can communicate with the outside world.","type":"text"}],"fragments":[{"kind":"keyword","text":"associatedtype"},{"text":" ","kind":"text"},{"kind":"identifier","text":"Action"}],"url":"\/documentation\/composablearchitecture\/reducer\/action"},"