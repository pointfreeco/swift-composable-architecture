alParam","text":"wrapped"},{"kind":"text","text":": () -> "},{"kind":"typeIdentifier","text":"Wrapped"},{"kind":"text","text":", "},{"kind":"externalParam","text":"fileID"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"StaticString","preciseIdentifier":"s:s12StaticStringV"},{"kind":"text","text":" = "},{"kind":"keyword","text":"#fileID"},{"kind":"text","text":", "},{"kind":"externalParam","text":"line"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UInt","preciseIdentifier":"s:Su"},{"kind":"text","text":" = "},{"kind":"keyword","text":"#line"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"_IfLetReducer"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"Self"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Wrapped"},{"kind":"text","text":"> "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"WrappedState"},{"kind":"text","text":" == "},{"kind":"typeIdentifier","text":"Wrapped"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"State"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"WrappedAction"},{"kind":"text","text":" == "},{"kind":"typeIdentifier","text":"Wrapped"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Action"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Wrapped"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP","text":"Reducer"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"parameters","parameters":[{"name":"toWrappedState","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A writable key path from parent state to a property containing optional"},{"type":"text","text":" "},{"type":"text","text":"child state."}]}]},{"name":"toWrappedAction","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A case path from parent action to a case containing child actions."}]}]},{"name":"wrapped","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that will be invoked with child actions against non-optional child"},{"type":"text","text":" "},{"type":"text","text":"state."}]}]}]},{"kind":"content","content":[{"anchor":"return-value","level":2,"type":"heading","text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"For example, if a parent feature holds onto a piece of optional child state, then it can"},{"type":"text","text":" "},{"type":"text","text":"perform its core logic "},{"type":"emphasis","inlineContent":[{"type":"text","text":"and"}]},{"type":"text","text":" the child’s logic by using the "},{"type":"codeVoice","code":"ifLet"},{"type":"text","text":" operator:"}]},{"type":"codeListing","syntax":"swift","code":["struct Parent: Reducer {","  struct State {","    var child: Child.State?","    \/\/ ...","  }","  enum Action {","    case child(Child.Action)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .ifLet(\\.child, action: \/Action.child) {","      Child()","    }","  }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"ifLet"},{"type":"text","text":" operator does a number of things to try to enforce correctness:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"It forces a specific order of operations for the child and parent features. It runs the"},{"type":"text","text":" "},{"type":"text","text":"child first, and then the parent. If the order was reversed, then it would be possible for"},{"type":"text","text":" "},{"type":"text","text":"the parent feature to "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" out the child state, in which case the child feature would not"},{"type":"text","text":" "},{"type":"text","text":"be able to react to that action. That can cause subtle bugs."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"It automatically cancels all child effects when it detects the child’s state is "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"’d"},{"type":"text","text":" "},{"type":"text","text":"out."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Automatically "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"s out child state when an action is sent for alerts and confirmation"},{"type":"text","text":" "},{"type":"text","text":"dialogs."}]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"See "},{"type":"reference","isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer\/ifLet(_:action:destination:fileID:line:)"},{"type":"text","text":" for a more advanced operator suited"},{"type":"text","text":" "},{"type":"text","text":"to navigation."}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/composablearchitecture\/reduce\/iflet(_:action:then:fileid:line:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reduce\/ifLet(_:action:then:fileID:line:)","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Embeds a child reducer in a parent domain that works on an optional property of parent state."}],"kind":"symbol","metadata":{"role":"symbol","title":"ifLet(_:action:then:fileID:line:)","roleHeading":"Instance Method","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind"