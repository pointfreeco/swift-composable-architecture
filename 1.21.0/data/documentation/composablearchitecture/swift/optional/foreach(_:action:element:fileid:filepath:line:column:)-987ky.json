ind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"Element","kind":"typeIdentifier"},{"text":" : ","kind":"text"},{"text":"Reducer","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","preciseIdentifier":"s:22ComposableArchitecture7ReducerP","kind":"typeIdentifier"},{"text":"\n","kind":"text"}],"platforms":["macOS"],"languages":["swift"]}],"kind":"declarations"},{"parameters":[{"content":[{"inlineContent":[{"type":"text","text":"A writable key path from parent state to an "},{"type":"codeVoice","code":"IdentifiedArray"},{"type":"text","text":" of child"},{"type":"text","text":" "},{"type":"text","text":"state."}],"type":"paragraph"}],"name":"toElementsState"},{"content":[{"type":"paragraph","inlineContent":[{"text":"A case path from parent action to an ","type":"text"},{"isActive":true,"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/IdentifiedAction","type":"reference"},{"text":" of child","type":"text"},{"text":" ","type":"text"},{"text":"actions.","type":"text"}]}],"name":"toElementAction"},{"content":[{"type":"paragraph","inlineContent":[{"text":"A reducer that will be invoked with child actions against elements of child","type":"text"},{"text":" ","type":"text"},{"text":"state.","type":"text"}]}],"name":"element"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The fileID."}]}],"name":"fileID"},{"content":[{"inlineContent":[{"text":"The filePath.","type":"text"}],"type":"paragraph"}],"name":"filePath"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The line."}]}],"name":"line"},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"The column."}]}],"name":"column"}],"kind":"parameters"},{"content":[{"type":"heading","text":"Return Value","level":2,"anchor":"return-value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}],"kind":"content"},{"content":[{"anchor":"discussion","type":"heading","text":"Discussion","level":2},{"inlineContent":[{"type":"text","text":"For example, if a parent feature holds onto an array of child states, then it can perform"},{"type":"text","text":" "},{"type":"text","text":"its core logic "},{"type":"emphasis","inlineContent":[{"text":"and","type":"text"}]},{"type":"text","text":" the childâ€™s logic by using the "},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" operator:"}],"type":"paragraph"},{"code":["@Reducer","struct Parent {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case rows(IdentifiedActionOf<Row>)","    \/\/ ...","  }","","  var body: some Reducer<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \\.rows) {","      Row()","    }","  }","}"],"type":"codeListing","syntax":"swift"},{"name":"Tip","style":"tip","content":[{"inlineContent":[{"type":"text","text":"We are using "},{"type":"codeVoice","code":"IdentifiedArray"},{"type":"text","text":" from our"},{"type":"text","text":" "},{"identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections","isActive":true,"type":"reference"},{"type":"text","text":" library because it provides a safe"},{"type":"text","text":" "},{"type":"text","text":"and ergonomic API for accessing elements from a stable ID rather than positional indices."}],"type":"paragraph"}],"type":"aside"},{"inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" forces a specific order of operations for the child and parent features. It"},{"type":"text","text":" "},{"type":"text","text":"runs the child first, and then the parent. If the order was reversed, then it would be"},{"type":"text","text":" "},{"type":"text","text":"possible for the parent feature to remove the child state from the array, in which case the"},{"type":"text","text":" "},{"type":"text","text":"child feature would not be able to react to that action. That can cause subtle bugs."}],"type":"paragraph"},{"inlineContent":[{"text":"It is still possible for a parent feature higher up in the application to remove the child","type":"text"},{"text":" ","type":"text"},{"text":"state from the array before the child has a chance to react to the action. In such cases a","type":"text"},{"text":" ","type":"text"},{"text":"runtime warning is shown in Xcode to let you know that thereâ€™s a potential problem.","type":"text"}],"type":"paragraph"}],"kind":"content"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/composablearchitecture\/swift\/optional\/foreach(_:action:element:fileid:filepath:line:column:)-987ky"]}],"identifier":{"url":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Swift\/Optional\/forEach(_:action:element:fileID:filePath:line:column:)-987ky","interfaceLanguage":"swift"},"sections":[],"hierarchy":{"paths":[["doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Reducer","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Swift\/Optional","doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/Swift\/Optional\/Reducer-Implementations"]]},"metadata":{"modules":[{"name":"ComposableArchitecture"}],"roleHeading":"Instance Method","extendedModule":"ComposableArchitecture","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"forEach","kind":"identifier"},{"text":"<","kind":"text"},{"text":"ElementState","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"ElementAction","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"ID","kind":"genericParameter"},{"text":", ","kind":"text"},{"text":"Element","kind":"genericParameter"},{"text":">(","kind":"text"},{"preciseIdentifier":"s:s15WritableKeyPa