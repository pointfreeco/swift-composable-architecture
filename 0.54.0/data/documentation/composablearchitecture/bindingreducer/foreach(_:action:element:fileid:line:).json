hitecture14ReducerBuilderO","text":"ReducerBuilder"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"ElementState"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"ElementAction"},{"kind":"text","text":">"},{"kind":"text","text":" "},{"kind":"externalParam","text":"element"},{"kind":"text","text":": () -> "},{"kind":"typeIdentifier","text":"Element"},{"kind":"text","text":", "},{"kind":"externalParam","text":"fileID"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"StaticString","preciseIdentifier":"s:s12StaticStringV"},{"kind":"text","text":" = "},{"kind":"keyword","text":"#fileID"},{"kind":"text","text":", "},{"kind":"externalParam","text":"line"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UInt","preciseIdentifier":"s:Su"},{"kind":"text","text":" = "},{"kind":"keyword","text":"#line"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"_ForEachReducer"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"Self"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"ID"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Element"},{"kind":"text","text":"> "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"ElementState"},{"kind":"text","text":" == "},{"kind":"typeIdentifier","text":"Element"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"State"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"ElementAction"},{"kind":"text","text":" == "},{"kind":"typeIdentifier","text":"Element"},{"kind":"text","text":"."},{"kind":"typeIdentifier","text":"Action"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"ID"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"Hashable","preciseIdentifier":"s:SH"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Element"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/ReducerProtocol","preciseIdentifier":"s:22ComposableArchitecture15ReducerProtocolP","text":"ReducerProtocol"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"parameters","parameters":[{"name":"toElementsState","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A writable key path from parent state to an "},{"type":"codeVoice","code":"IdentifiedArray"},{"type":"text","text":" of child"},{"type":"text","text":" "},{"type":"text","text":"state."}]}]},{"name":"toElementAction","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A case path from parent action to child identifier and child actions."}]}]},{"name":"element","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that will be invoked with child actions against elements of child"},{"type":"text","text":" "},{"type":"text","text":"state."}]}]}]},{"kind":"content","content":[{"anchor":"return-value","level":2,"type":"heading","text":"Return Value"},{"type":"paragraph","inlineContent":[{"type":"text","text":"A reducer that combines the child reducer with the parent reducer."}]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"For example, if a parent feature holds onto an array of child states, then it can perform"},{"type":"text","text":" "},{"type":"text","text":"its core logic "},{"type":"emphasis","inlineContent":[{"type":"text","text":"and"}]},{"type":"text","text":" the child’s logic by using the "},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" operator:"}]},{"type":"codeListing","syntax":"swift","code":["struct Parent: ReducerProtocol {","  struct State {","    var rows: IdentifiedArrayOf<Row.State>","    \/\/ ...","  }","  enum Action {","    case row(id: Row.State.ID, action: Row.Action)","    \/\/ ...","  }","","  var body: some ReducerProtocol<State, Action> {","    Reduce { state, action in","      \/\/ Core logic for parent feature","    }","    .forEach(\\.rows, action: \/Action.row) {","      Row()","    }","  }","}"]},{"style":"tip","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"We are using "},{"type":"codeVoice","code":"IdentifiedArray"},{"type":"text","text":" from our"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"http:\/\/github.com\/pointfreeco\/swift-identified-collections"},{"type":"text","text":" library because it provides a safe"},{"type":"text","text":" "},{"type":"text","text":"and ergonomic API for accessing elements from a stable ID rather than positional indices."}]}],"type":"aside","name":"Tip"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"forEach"},{"type":"text","text":" forces a specific order of operations for the child and parent features. It"},{"type":"text","text":" "},{"type":"text","text":"runs the child first, and then the parent. If the order was reversed, then it would be"},{"type":"text","text":" "},{"type":"text","text":"possible for the parent feature to remove the child state from the array, in which case the"},{"type":"text","text":" "},{"type":"text","text":"child feature would not be able to react to that action. That can cause subtle bugs."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"It is still possible for a parent feature higher up in the application to remove the child"},{"type":"text","text":" "},{"type":"text","text":"state from the array before the child has a chance to react to the action. In such cases a"},{"type":"text","text":" "},{"type":"text","text":"runtime warning is shown in Xcode to let you know that there’s a potential problem."}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/composablearchitecture\/bindingreducer\/foreach(_:action:element:fileid:line:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier"