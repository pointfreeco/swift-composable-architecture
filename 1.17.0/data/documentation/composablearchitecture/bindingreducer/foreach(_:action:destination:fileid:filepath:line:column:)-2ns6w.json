ion {","    case path(StackActionOf<Path>)","    \/\/ ...","  }","  var body: some ReducerOf<Self> {","    Reduce { state, action in","      \/\/ Core parent logic","    }","    .forEach(\\.path, action: \\.path) {","      Path()","    }","  }","}"]},{"inlineContent":[{"type":"text","text":"The "},{"code":"forEach","type":"codeVoice"},{"type":"text","text":" operator does a number of things to make integrating parent and child features"},{"text":" ","type":"text"},{"text":"ergonomic and enforce correctness:","type":"text"}],"type":"paragraph"},{"items":[{"content":[{"inlineContent":[{"type":"text","text":"It forces a specific order of operations for the child and parent features:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"When a "},{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/element(id:action:)","type":"reference","isActive":true},{"text":" action is sent it runs the","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"child first, and then the parent. If the order was reversed, then it would be possible"},{"type":"text","text":" "},{"type":"text","text":"for the parent feature to "},{"code":"nil","type":"codeVoice"},{"type":"text","text":" out the child state, in which case the child feature"},{"type":"text","text":" "},{"text":"would not be able to react to that action. That can cause subtle bugs.","type":"text"}]}]},{"content":[{"inlineContent":[{"type":"text","text":"When a "},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/popFrom(id:)","isActive":true},{"type":"text","text":" action is sent it runs the parent feature"},{"text":" ","type":"text"},{"text":"before the child state is popped off the stack. This gives the parent feature an","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"opportunity to inspect the child state one last time before the state is removed."}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"When a "},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction\/push(id:state:)","isActive":true},{"type":"text","text":" action is sent it runs the parent feature"},{"text":" ","type":"text"},{"text":"after the child state is appended to the stack. This gives the parent feature an","type":"text"},{"type":"text","text":" "},{"text":"opportunity to make extra mutations to the state after it has been added.","type":"text"}]}]}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"It automatically cancels all child effects when it detects the child’s state is removed","type":"text"},{"text":" ","type":"text"},{"text":"from the stack","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"It gives the child feature access to the ","type":"text"},{"type":"reference","identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/DismissEffect","isActive":true},{"type":"text","text":" dependency, which allows the"},{"text":" ","type":"text"},{"type":"text","text":"child feature to dismiss itself without communicating with the parent."}]}]}],"type":"unorderedList"}],"kind":"content"}],"references":{"doc://ComposableArchitecture/documentation/ComposableArchitecture/StackAction":{"identifier":"doc:\/\/ComposableArchitecture\/documentation\/ComposableArchitecture\/StackAction","title":"StackAction","role":"symbol","kind":"symbol","type":"topic","abstract":[{"type":"text","text":"A wrapper type for actions that can be presented in a navigation stack."}],"fragments":[{"kind":"keyword","text":"enum"},{"text":" ","kind":"text"},{"kind":"identifier","text":"StackAction"}],"url":"\/documentation\/composablearchitecture\/stackaction","navigatorTitle":[{"kind":"identifier","text":"StackAction"}]},"doc://ComposableArchitecture/documentati